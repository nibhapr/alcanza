---
import Layout from "../../layouts/Layout.astro";
import MarineEquipmentsComponent from "../../components/services/marine-equipments.astro";

// SEO Meta Data
const title =
  "Marine Supplies & Equipment in UAE | Leading Ship Chandler in Dubai";
const description =
  "Premier marine equipment supplier in UAE. Specialized in ship chandling services and technical supplies across Dubai, Abu Dhabi, Fujairah and all UAE ports. Contact us today.";
const canonicalURL = new URL("/services/marine-equipments", Astro.site);
const alternateArabicURL = new URL("/services/marine-equipments", Astro.site);
---

<Layout title={title} description={description} canonicalURL={canonicalURL}>
  <!-- Additional Meta Tags -->
  <meta slot="head" />
  <meta
    name="keywords"
    content="marine equipment UAE, ship supplies UAE, marine technical supplies UAE, ship chandling services uae, ship chandling services dubai, ship chandling services saudi arabia, ship chandling services other uae countries, marine equipment supplier UAE, Alcanza Ship Services UAE, marine spare parts UAE, ship chandler UAE, marine engineering supplies UAE, marine safety equipment UAE"
  />
  <meta
    name="robots"
    content="index, follow, max-image-preview:large, max-snippet:-1, max-video-preview:-1"
  />
  <meta name="geo.region" content="AE-DU" />
  <meta name="geo.placename" content="Dubai" />
  <meta name="geo.position" content="25.2848;55.3222" />
  <meta name="ICBM" content="25.2848, 55.3222" />

  <!-- Open Graph Tags -->
  <meta property="og:title" content={title} />
  <meta property="og:description" content={description} />
  <meta property="og:type" content="website" />
  <meta property="og:url" content={canonicalURL} />
  <meta property="og:locale" content="en_AE" />
  <meta property="og:site_name" content="Alcanza Ship Services" />
  <meta
    property="og:image"
    content="https://alcanzaship.com/services/marine-equipments.jpg"
  />
  <meta property="og:image:width" content="1200" />
  <meta property="og:image:height" content="630" />

  <!-- Twitter Card Tags -->
  <meta name="twitter:card" content="summary_large_image" />
  <meta name="twitter:title" content={title} />
  <meta name="twitter:description" content={description} />
  <meta
    name="twitter:image"
    content="https://alcanzaship.com/services/marine-equipments.jpg"
  />

  <!-- Language Alternates -->
  <link rel="alternate" hreflang="en-AE" href={canonicalURL} />
  <link rel="alternate" hreflang="ar-AE" href={alternateArabicURL} />

  <!-- Schema.org JSON-LD -->
  <script type="application/ld+json">
    {
      "@context": "https://schema.org",
      "@graph": [
        {
          "@type": "MarineSupplyStore",
          "@id": "https://alcanzaship.com",
          "name": "Alcanza Ship Services",
          "description": "Professional marine equipment and supplies provider in Dubai, UAE",
          "url": "https://alcanzaship.com",
          "telephone": "+971561635323",
          "email": ["sales@alcanzaship.com", "operation@alcanzaship.com"],
          "address": {
            "@type": "PostalAddress",
            "streetAddress": "Mai Tower, Office 401-405, Al Nahda 1",
            "addressLocality": "Dubai",
            "addressRegion": "Dubai",
            "addressCountry": "AE",
            "postalCode": "00000"
          },
          "geo": {
            "@type": "GeoCoordinates",
            "latitude": "25.2848",
            "longitude": "55.3222"
          },
          "openingHours": ["Mo-Fr 09:00-18:00"],
          "priceRange": "$$",
          "image": {
            "@type": "ImageObject",
            "url": "https://alcanzaship.com/home/logo.png",
            "width": "512",
            "height": "512"
          },
          "sameAs": [
            "https://www.linkedin.com/company/alcanza-ship-services",
            "https://www.facebook.com/alcanzashipservices",
            "https://www.instagram.com/alcanzaship",
            "https://www.twitter.com/alcanzaship"
          ],
          "areaServed": {
            "@type": "GeoCircle",
            "geoMidpoint": {
              "@type": "GeoCoordinates",
              "latitude": "25.2848",
              "longitude": "55.3222"
            },
            "geoRadius": "500000"
          },
          "hasOfferCatalog": {
            "@type": "OfferCatalog",
            "name": "Marine Equipment Services in UAE",
            "itemListElement": [
              {
                "@type": "Offer",
                "itemOffered": {
                  "@type": "Service",
                  "name": "Marine Technical Supplies",
                  "description": "Complete range of marine technical supplies and equipment across UAE ports"
                }
              },
              {
                "@type": "Offer",
                "itemOffered": {
                  "@type": "Service",
                  "name": "Ship Chandling Services",
                  "description": "Professional ship chandling services in Dubai, Abu Dhabi, Fujairah and all UAE ports"
                }
              }
            ]
          },
          "contactPoint": [
            {
              "@type": "ContactPoint",
              "telephone": "+971561635323",
              "contactType": "sales",
              "email": "sales@alcanzaship.com",
              "availableLanguage": ["English", "Arabic"]
            },
            {
              "@type": "ContactPoint",
              "telephone": "+971561635323",
              "contactType": "customer service",
              "email": "operation@alcanzaship.com",
              "availableLanguage": ["English", "Arabic"]
            }
          ]
        },
        {
          "@type": "WebPage",
          "@id": "https://alcanzaship.com/services/marine-equipments",
          "url": "https://alcanzaship.com/services/marine-equipments",
          "name": "Marine Supplies & Equipment in UAE",
          "isPartOf": { "@id": "https://alcanzaship.com" },
          "datePublished": "2024-01-01",
          "dateModified": "2024-03-19",
          "breadcrumb": {
            "@type": "BreadcrumbList",
            "itemListElement": [
              {
                "@type": "ListItem",
                "position": 1,
                "item": {
                  "@id": "https://alcanzaship.com",
                  "name": "Home"
                }
              },
              {
                "@type": "ListItem",
                "position": 2,
                "item": {
                  "@id": "https://alcanzaship.com/services",
                  "name": "Services"
                }
              },
              {
                "@type": "ListItem",
                "position": 3,
                "item": {
                  "@id": "https://alcanzaship.com/services/marine-equipments",
                  "name": "Marine Equipment"
                }
              }
            ]
          }
        }
      ]
    }
  </script>

  <MarineEquipmentsComponent />
</Layout>
